{
  "compilerOptions": {
    "jsx": "react",
    "jsxFactory": "Nexora",
    "jsxFragmentFactory": "Nexora.Fragment",
    "outDir": "./dist",
    "module": "ESNext",
    "target": "ESNext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true
  },
  "include": ["*.tsx"],
  "exclude": ["node_modules"]
}
